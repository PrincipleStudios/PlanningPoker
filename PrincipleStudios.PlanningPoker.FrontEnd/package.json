    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "loglevel": "^1.8.0",
    "postcss": "^8.4.7",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.23",
