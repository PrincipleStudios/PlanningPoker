{
  "name": "ps-planning-poker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "touch node_modules/_._",
    "generate-openapi": "openapi-codegen-typescript ../schemas/api.yaml src/api/",
    "build": "next build && next export",
    "build:dotnet": "next build && next export -o ../PrincipleStudios.PlanningPoker/wwwroot",
    "start": "next dev",
